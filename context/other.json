 {
 	"description": "Events made in other context",
 	"$schema": "http://api.contactlab.it/hub#",
 	"type": "object",
 	"additionalProperties": false,
 	"properties": {
 		"client": {
 			"type": "object",
 			"properties": {
 				"userAgent": {
 					"type": [
 						"string",
 						"null"
 					]
 				},
 				"ip": {
 					"type": "string",
 					"format": "ipv4"
 				},
 				"localization": {
 					"type": [
 						"object",
 						"null"
 					],
 					"properties": {
 						"city": {
 							"type": "string"
 						},
 						"country": {
 							"type": "string"
 						},
 						"region": {
 							"type": "string"
 						},
 						"province": {
 							"type": "string"
 						},
 						"zip": {
 							"type": "string"
 						},
 						"geo": {
 							"type": "object",
 							"properties": {
 								"lat": {
 									"type": "number",
 									"minimum": -90,
 									"maximum": 90,
 									"description": "latitude"
 								},
 								"lon": {
 									"type": "number",
 									"minimum": -180,
 									"maximum": 180,
 									"description": "longitude"
 								}
 							}
 						}
 					}
 				}
 			}
 		},
 		"user": {
 			"type": "object",
 			"properties": {
 				"id": {
 					"description": "The identifier of the user",
 					"type": "string"
 				},
 				"externalId": {
 					"description": "The external identifier of the user",
 					"type": [
 						"string",
 						"null"
 					]
 				},
 				"username": {
 					"description": "The username of the user",
 					"type": [
 						"string",
 						"null"
 					]
 				},
 				"firstName": {
 					"description": "The first name of the user",
 					"type": [
 						"string",
 						"null"
 					]
 				},
 				"lastName": {
 					"description": "The last name of the user",
 					"type": [
 						"string",
 						"null"
 					]
 				},
 				"contacts": {
 					"description": "The contact information for the user",
 					"type": [
 						"object",
 						"null"
 					],
 					"properties": {
 						"email": {
 							"description": "The email address of the user",
 							"type": [
 								"string",
 								"null"
 							],
 							"format": "email"
 						},
 						"mobilePhone": {
 							"description": "The mobile phone number of the user",
 							"type": [
 								"string",
 								"null"
 							]
 						},
 						"phone": {
 							"description": "The phone number of the user",
 							"type": [
 								"string",
 								"null"
 							]
 						}
 					}
 				}
 			},
 			"required": [
 				"id"
 			]
 		}
 	}
 }
