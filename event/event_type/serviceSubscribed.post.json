{
  "$schema": "http://api.contactlab.it/hub#",
  "type": "object",
  "description": "Customer subscribed to a service subscription",
  "properties": {
    "customerId": {
      "type": "string",
      "pattern": "^.+$"
    },
    "bringBackProperties": {
      "$ref": "resource:/public/schema/enums.json#/definitions/EventBringBackProperties"
    },
    "type": {
      "$ref": "resource:/public/schema/enums.json#/definitions/EventType"
    },
    "context": {
      "$ref": "resource:/public/schema/enums.json#/definitions/EventContext"
    },
    "date": {
      "type": "string",
      "format": "date-time"
    },
    "contextInfo": {
      "$ref": "resource:/public/schema/enums.json#/definitions/EventContextInfo"
    },
    "properties": {
      "type": "object",
      "properties": {
        "subscriberId": {
          "type": "string",
          "description": "An ID used to identify the subscriber"
        },
        "serviceId": {
          "type": "string",
          "description": "The idenfier of the service"
        },
        "serviceName": {
          "type": "string",
          "description": "The name of the service"
        },
        "serviceType": {
          "type": "string",
          "description": "The type of service"
        },
        "startDate": {
          "type": "string",
          "description": "Date time of the start of service subscription",
          "format": "date-time"
        },
        "endDate": {
          "type": "string",
          "description": "Date time of the end of service subscription",
          "format": "date-time"
        },
        "extraProperties": {
          "type": "object",
          "description": "Extra pieces of information you can tie to events you track"
        }
      }
    }
  },
  "required": [
    "type",
    "context"
  ],
  "additionalProperties": false
}