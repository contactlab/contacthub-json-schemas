{
  "$schema": "http://api.contactlab.it/hub#",
  "type": "object",
  "properties": {
    "customerId": {
      "type": "string",
      "pattern": "^.+$"
    },
    "bringBackProperties": {
      "$ref": "resource:/public/schema/enums.json#/definitions/EventBringBackProperties"
    },
    "type": {
      "$ref": "resource:/public/schema/enums.json#/definitions/EventType"
    },
    "context": {
      "$ref": "resource:/public/schema/enums.json#/definitions/EventContext"
    },
    "date": {
      "type": "string",
      "format": "date-time"
    },
    "contextInfo": {
      "$ref": "resource:/public/schema/enums.json#/definitions/EventContextInfo"
    },
    "properties": {
      "type": "object",
      "properties": {
        "orderId": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "carrier": {
          "type": "string"
        },
        "trackingCode": {
          "type": "string"
        },
        "trackingUrl": {
          "type": "string",
          "format": "uri"
        },
        "weight": {
          "type": "number"
        },
        "packages": {
          "type": "integer"
        }
      }
    }
  },
  "required": [
    "type",
    "context"
  ],
  "additionalProperties": false
}