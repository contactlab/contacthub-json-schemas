{
  "$schema": "http://api.contactlab.it/hub#",
  "type": "object",
  "description": "Customer subscribed a list",
  "properties": {
    "customerId": {
      "type": "string",
      "pattern": "^.+$"
    },
    "bringBackProperties": {
      "$ref": "resource:/public/schema/enums.json#/definitions/EventBringBackProperties"
    },
    "type": {
      "$ref": "resource:/public/schema/enums.json#/definitions/EventType"
    },
    "context": {
      "$ref": "resource:/public/schema/enums.json#/definitions/EventContext"
    },
    "date": {
      "type": "string",
      "format": "date-time"
    },
    "contextInfo": {
      "$ref": "resource:/public/schema/enums.json#/definitions/EventContextInfo"
    },
    "properties": {
      "type": "object",
      "properties": {
        "subscriberId": {
          "type": "string",
          "description": "An ID used to identify the subscriber"
        },
        "listId": {
          "type": "string",
          "description": "Id used to identify a list"
        },
        "listName": {
          "type": "string",
          "description": "Name used to identify a list"
        },
        "channel": {
          "type": "string",
          "description": "The campaign medium",
          "enum": [
            "SMS",
            "EMAIL",
            "PUSH",
            "FAX"
          ]
        },
        "extraProperties": {
          "type": "object",
          "description": "Extra pieces of information you can tie to events you track"
        }
      }
    }
  },
  "required": [
    "type",
    "context"
  ],
  "additionalProperties": false
}